{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://api.raphnesia.my.id/api/v1",
    "NEXT_PUBLIC_USE_PROXY": "true",
    "NEXT_PUBLIC_SITE_URL": "https://raphnesia.my.id"
  },
  "functions": {
    "src/app/api/proxy/[...path]/route.ts": {
      "maxDuration": 30
    }
  }
} 